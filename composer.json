{
    "name": "dragon-code/env-sync",
    "description": "Keeping your .env.example is up to date",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "env",
        "dotenv",
        "environment",
        "sync",
        "laravel",
        "symfony",
        "yii",
        "yii2",
        "framework",
        "native",
        "dragon-code",
        "dragon",
        "andrey-helldar"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "support": {
        "issues": "https://github.com/TheDragonCode/env-sync/issues",
        "source": "https://github.com/TheDragonCode/env-sync"
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "dragon-code/support": "^5.7",
        "symfony/finder": "^4.0 || ^5.0 || ^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^4.0 || ^5.0 || ^6.0"
    },
    "suggest": {
        "dragon-code/env-sync-laravel": "env-sync package adapter for Laravel framework"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "DragonCode\\EnvSync\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
