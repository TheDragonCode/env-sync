{
    "name": "andrey-helldar/env-sync",
    "description": "Keeping your .env.example is up to date",
    "type": "library",
    "license": "MIT",
    "keywords": ["env", "dotenv", "environment", "sync", "laravel", "symfony", "yii", "yii2", "framework"],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "support": {
        "issues": "https://github.com/andrey-helldar/env-sync/issues",
        "source": "https://github.com/andrey-helldar/env-sync"
    },
    "require": {
        "php": "^7.3|^8.0",
        "andrey-helldar/support": "^2.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\EnvSync\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Helldar\\EnvSync\\ServiceProvider"
            ]
        }
    }
}
